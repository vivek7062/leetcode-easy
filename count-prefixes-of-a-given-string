// Question - https://leetcode.com/problems/count-prefixes-of-a-given-string/
// Answer in cpp



class Solution {
public:
    int countPrefixes(vector<string>& words, string s) {
        int ans=0;
        for(int i=0;i<words.size();i++){
            if(!s.find(words[i])) ans++;
        }
        return ans;
    }
};
