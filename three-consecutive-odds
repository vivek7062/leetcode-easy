// Question - https://leetcode.com/problems/three-consecutive-odds/
// Answer in cpp

class Solution {
public:
    bool threeConsecutiveOdds(vector<int>& arr) {
        int count=0;
        for(auto& num:arr){
            if(num%2!=0) count++;
            else count=0;
            if(count==3) return true;
        }
        return false;
    }
};
